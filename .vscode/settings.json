{
    "protoc": {
        "path": "../protobuf",
        "compile_on_save": false,
        "options": [
            "--go_out=../generated/proto/go",
            "--go_opt=paths=source_relative",
            "--go-grpc_out=../generated/proto/go",
            "--go-grpc_opt=paths=source_relative",
            "--proto_path=${workspaceRoot}/protobuf",
            // "--proto_path=${env.GOPATH}/src",
        ]
    }
}
